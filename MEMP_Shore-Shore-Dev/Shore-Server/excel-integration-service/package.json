{
  "name": "excel-integration-service",
  "version": "1.0.0",
  "description": "Microservice for Excel import and export of vessel reports.",
  "main": "excelIntegrationServer.js",
  "type": "module",
  "scripts": {
    "start": "node excelIntegrationServer.js",
    "dev": "nodemon excelIntegrationServer.js",
    "setup:cert": "node generate-cert.js",
    "manifest:dev": "node generate-manifest.js",
    "manifest:prod": "cross-env NODE_ENV=production node generate-manifest.js",
    "prestart": "npm run manifest:dev"
  },
  "keywords": [
    "excel",
    "import",
    "export",
    "reports"
  ],
  "author": "Viswa Digital",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "formidable": "^3.5.4",
    "helmet": "^8.1.0",
    "moment-timezone": "^0.6.0",
    "mssql": "^10.0.2",
    "multer": "^1.4.5-lts.1",
    "selfsigned": "^2.4.1"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.0"
  }
}
